{
  "name": "@bureaublauwgeel/browser-soap",
  "version": "1.0.4",
  "description": "A minimal SOAP client in javascript, so tiny it even runs in browser",
  "engines": {
    "node": ">=0.8.0"
  },
  "author": "Koen Klasing <info@bureaublauwgeel.nl>",
  "dependencies": {
    "node-xml": "1.0.2"
  },
  "repository": {
